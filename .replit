modules = ["nodejs-20", "bash", "web", "postgresql-16", "python-3.11"]
[agent]
expertMode = true
integrations = ["javascript_openai_ai_integrations:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["glib", "glibcLocales", "libxcrypt"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "NRL Fantasy API"

[[workflows.workflow]]
name = "NRL Fantasy API"
